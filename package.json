{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate using React, Redux, Redux Saga, Immutable.js, Webpack, etc.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ./index.js & npm run style-watch",
    "build": "webpack --verbose --colors --display-error-details --config build.config.js && npm run style",
    "lint": "eslint ./; exit 0",
    "test": "mocha --require babel-core/register",
    "style": "$(npm bin)/stylus src/styles/style.styl -o static/style.css -I static -u autoprefixer-stylus --with \"{ browsers: ['last 3 versions', 'ie >= 10'] }\"",
    "style-watch": "$(npm bin)/stylus -w src/styles/style.styl -o static/style.css -I static -u autoprefixer-stylus --with \"{ browsers: ['last 3 versions', 'ie >= 10'] }\"",
    "generate": "node ./generate",
    "g": "npm run generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/satellitejs/react-boilerplate.git"
  },
  "keywords": [
    "React",
    "react",
    "Redux",
    "redux",
    "saga",
    "stylus",
    "boilerplate",
    "immutable"
  ],
  "author": "satellitejs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/satellitejs/react-boilerplate/issues"
  },
  "homepage": "https://github.com/satellitejs/react-boilerplate#readme",
  "dependencies": {
    "axios": "^0.11.0",
    "babel": "^6.5.2",
    "babel-polyfill": "^6.8.0",
    "babel-runtime": "^6.6.1",
    "babel-register": "^6.8.0",
    "express": "^4.13.4",
    "immutable": "^3.8.1",
    "normalizr": "^2.0.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router5": "^3.1.0",
    "redux": "^3.5.2",
    "redux-actions": "^0.9.1",
    "redux-immutable": "^3.0.6",
    "redux-router5": "^3.0.0",
    "redux-saga": "^0.10.2",
    "reselect": "^2.5.1",
    "router5": "^3.0.3",
    "router5-history": "^3.0.3"
  },
  "devDependencies": {
    "autoprefixer-stylus": "^0.9.3",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-espower": "^2.1.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.9.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-react": "^5.0.1",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "nock": "^8.0.0",
    "power-assert": "^1.4.0",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "stylus": "^0.54.5",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
